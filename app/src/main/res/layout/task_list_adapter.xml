<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:clickable="true"
    android:focusable="true">

    <data>

        <import type="android.view.View" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?android:attr/selectableItemBackground">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardTasks"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardElevation="2dp"
            app:cardMaxElevation="4dp"
            app:cardUseCompatPadding="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/lnrHeader"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:orientation="horizontal"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent">


                    <LinearLayout
                        android:id="@+id/lnr_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp"
                        app:layout_constraintLeft_toRightOf="@id/lnrMST"
                        app:layout_constraintTop_toTopOf="parent">


                        <TextView
                            android:id="@+id/txt_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:lineSpacingMultiplier="1.2"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="15sp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:ignore="MissingConstraints" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lnr_Map"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginTop="4dp"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:gravity="center"
                        app:layout_constraintLeft_toRightOf="@id/lnr_time"
                        app:layout_constraintTop_toTopOf="parent">


                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/img_tracklocation"
                            android:layout_width="28dp"
                            android:layout_height="28dp"
                            android:layout_gravity="center"
                            app:srcCompat="@drawable/ic_compass" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/btnHelpline"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginTop="4dp"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:gravity="center"
                        app:layout_constraintLeft_toRightOf="@id/lnr_Map"
                        app:layout_constraintTop_toTopOf="parent">

                        <ImageView
                            android:id="@+id/helpline"
                            android:layout_width="28dp"
                            android:layout_height="28dp"
                            android:gravity="center"
                            app:srcCompat="@drawable/ic_helpcall" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/img_warning"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginLeft="3dp"
                        android:layout_marginTop="4dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:gravity="center"
                        android:visibility="gone"
                        app:layout_constraintLeft_toRightOf="@id/btnHelpline"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/warning"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_gravity="center"
                            app:srcCompat="@drawable/ic_running" />


                    </LinearLayout>

                </LinearLayout>


                <androidx.cardview.widget.CardView
                    android:id="@+id/lnrStatus"
                    android:layout_width="85dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="5dp"
                    app:cardBackgroundColor="@color/colorPrimaryDark"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="2dp"
                    app:cardUseCompatPadding="true"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="3dp"
                        android:layout_marginRight="3dp"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="11sp" />

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:id="@+id/lnrDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/lnrStatus">


                    <TextView
                        android:id="@+id/txtDate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:layout_marginRight="5dp"
                        android:lineSpacingMultiplier="1.2"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="MissingConstraints" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/lnrName"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:orientation="horizontal"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/lnrHeader">

                    <LinearLayout
                        android:id="@+id/lnrMST"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginRight="5dp"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:gravity="center"
                        android:visibility="gone">


                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgMST"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_gravity="center"
                            app:srcCompat="@drawable/ic_link" />


                    </LinearLayout>


                    <TextView
                        android:id="@+id/txt_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:inputType="textPersonName"
                        android:lineSpacingMultiplier="1.2"
                        android:text="Mr. Arjun Bhatt"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/lnr_time"
                        tools:ignore="MissingConstraints" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/lnrMain"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/lnrName">


                    <LinearLayout
                        android:id="@+id/lnrDetail"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">


                        <LinearLayout
                            android:id="@+id/lnrTask"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/lnr_service"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="8dp"

                                >

                                <TextView
                                    android:id="@+id/labelPlan"
                                    android:layout_width="130dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/service_plan"
                                    android:textSize="14sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=":"
                                    android:textSize="14sp" />

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="15dp"
                                    android:gravity="right">

                                    <TextView
                                        android:id="@+id/txt_service"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:ellipsize="end"
                                        android:gravity="left"
                                        android:maxLines="2"
                                        android:textColor="@color/black"
                                        android:textSize="14sp" />

                                </RelativeLayout>


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lnr_tag"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="2dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/lnr_service">

                                <TextView
                                    android:id="@+id/labelTag"
                                    android:layout_width="130dp"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"
                                    android:text="@string/tag"
                                    android:textSize="14sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=":"
                                    android:textSize="14sp" />

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="15dp"
                                    android:gravity="left">

                                    <TextView
                                        android:id="@+id/txt_tag"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:textColor="@color/black"
                                        android:textSize="14sp" />
                                </RelativeLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lnr_sequence"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="2dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/lnr_tag">

                                <TextView
                                    android:id="@+id/labelSequence"
                                    android:layout_width="130dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/sequence_no"
                                    android:textSize="14sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=":"
                                    android:textSize="14sp" />

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="15dp"
                                    android:gravity="left">

                                    <TextView
                                        android:id="@+id/txt_sequence"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:textColor="@color/black"
                                        android:textSize="14sp" />

                                </RelativeLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lnr_type"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="2dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/lnr_sequence">

                                <TextView
                                    android:id="@+id/labelType"
                                    android:layout_width="130dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/type"
                                    android:textSize="14sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=":"
                                    android:textSize="14sp" />

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="15dp"
                                    android:gravity="left">

                                    <TextView
                                        android:id="@+id/txt_type"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:ellipsize="end"
                                        android:lines="1"
                                        android:textColor="@color/black"
                                        android:textSize="14sp" />
                                </RelativeLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lnr_address"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="2dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/lnr_type">

                                <TextView
                                    android:id="@+id/labelAddress"
                                    android:layout_width="130dp"
                                    android:layout_height="wrap_content"
                                    android:text="@string/address"
                                    android:textSize="14sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=":"
                                    android:textSize="14sp" />


                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginRight="15dp"
                                    android:gravity="left">

                                    <TextView
                                        android:id="@+id/txt_address"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:textColor="@color/black"
                                        android:textSize="14sp" />
                                </RelativeLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="2.5"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:id="@+id/lnr_landmark"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="15dp"
                                        android:layout_marginTop="2dp"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintTop_toBottomOf="@id/lnr_address">

                                        <TextView
                                            android:id="@+id/labelLandmark"
                                            android:layout_width="130dp"
                                            android:layout_height="wrap_content"
                                            android:text="@string/landmark"
                                            android:textSize="14sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text=":"
                                            android:textSize="14sp" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="right"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="15dp"
                                            android:gravity="left">

                                            <TextView
                                                android:id="@+id/txt_Landmark"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentRight="true"
                                                android:textColor="@color/black"
                                                android:textSize="14sp" />
                                        </RelativeLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/lnr_postalcode"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="15dp"
                                        android:layout_marginTop="2dp"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintTop_toBottomOf="@id/lnr_landmark">

                                        <TextView
                                            android:id="@+id/labelPostal"
                                            android:layout_width="130dp"
                                            android:layout_height="match_parent"
                                            android:text="@string/postal_code"
                                            android:textSize="14sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text=":"
                                            android:textSize="14sp" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="right"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="15dp"
                                            android:gravity="left">

                                            <TextView
                                                android:id="@+id/txt_postcode"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentRight="true"
                                                android:textColor="@color/black"
                                                android:textSize="14sp" />
                                        </RelativeLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/lnr_order"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="15dp"
                                        android:layout_marginTop="2dp"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintTop_toBottomOf="@id/lnr_postalcode">

                                        <TextView
                                            android:id="@+id/labelOrder"
                                            android:layout_width="130dp"
                                            android:layout_height="wrap_content"
                                            android:text="@string/order_no"
                                            android:textSize="14sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text=":"
                                            android:textSize="14sp" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="right"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="15dp"
                                            android:gravity="left">

                                            <TextView
                                                android:id="@+id/txt_orderno"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentRight="true"
                                                android:ellipsize="end"
                                                android:lines="1"
                                                android:maxLines="2"
                                                android:textColor="@color/black"
                                                android:textSize="14sp" />
                                        </RelativeLayout>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/lnr_amount"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="15dp"
                                        android:layout_marginTop="2dp"
                                        android:orientation="horizontal"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintTop_toBottomOf="@id/lnr_order">

                                        <TextView
                                            android:id="@+id/labelAmount"
                                            android:layout_width="130dp"
                                            android:layout_height="wrap_content"
                                            android:text="@string/amount_to_collect"
                                            android:textSize="14sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text=":"
                                            android:textSize="14sp" />

                                        <RelativeLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="right"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="15dp"
                                            android:gravity="left">

                                            <TextView
                                                android:id="@+id/txt_amount"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_alignParentRight="true"
                                                android:textColor="@color/black"
                                                android:textSize="14sp" />
                                        </RelativeLayout>

                                        <LinearLayout
                                            android:id="@+id/lnr_timer"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_marginRight="10dp"
                                            android:orientation="horizontal"
                                            android:visibility="gone">

                                            <androidx.appcompat.widget.AppCompatTextView
                                                android:id="@+id/txt_timer"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="right"
                                                android:gravity="right"
                                                android:text="@string/running_late"
                                                android:textColor="@color/red"
                                                android:textSize="16sp" />


                                        </LinearLayout>

                                    </LinearLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/lnrPartnerPic"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="right"
                                    android:layout_weight="0.8"
                                    android:gravity="right|bottom"
                                    android:orientation="vertical"
                                    android:visibility="gone">

                                    <de.hdodenhof.circleimageview.CircleImageView
                                        android:id="@+id/imgPartner"
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"
                                        android:layout_gravity="center"
                                        android:layout_margin="2dp"
                                        android:elevation="4dp"
                                        app:civ_border_color="@color/smoke_gray"
                                        app:civ_border_width="0.001dp"
                                        app:srcCompat="@drawable/ic_profile" />

                                    <TextView
                                        android:id="@+id/txtHelperName"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="1dp"
                                        android:gravity="center"
                                        android:textColor="@color/black"
                                        android:textSize="10sp" />

                                </LinearLayout>


                            </LinearLayout>


                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>


                <View
                    android:id="@+id/view"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/tab_textColor"
                    android:layerType="software"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/lnrMain" />


                <LinearLayout
                    android:id="@+id/layout_btns"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"

                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/view">

                    <LinearLayout
                        android:id="@+id/dispatch_task_mobile_no"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            app:srcCompat="@drawable/ic_mobile" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="@string/mobile_no"
                            android:lines="1"
                            android:ellipsize="end"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/tab_textColor" />


                    <LinearLayout
                        android:id="@+id/dispatch_task_alt_mobile_no"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            app:srcCompat="@drawable/ic_mobile" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="@string/alt_mobile_no"
                            android:lines="1"
                            android:ellipsize="end"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="12sp" />

                    </LinearLayout>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/tab_textColor" />

                    <LinearLayout
                        android:id="@+id/dispatch_task_phone_no"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:gravity="center"
                        android:orientation="horizontal">


                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            app:srcCompat="@drawable/ic_oldphone" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="@string/phone_no"
                            android:lines="1"
                            android:ellipsize="end"
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="12sp" />


                    </LinearLayout>


                </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>